{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "server": "cross-env DATABASE_URL=$(heroku config:get DATABASE_URL -a eventscape-dev) NODE_ENV=development nodemon server.js",
    "server-local": "cross-env DATABASE_URL=postgresql://localhost/eventscapelocal IS_LOCAL=true NODE_ENV=development nodemon server.js",
    "server-withproddb": "cross-env DATABASE_URL=$(heroku config:get DATABASE_URL -a eventscapeio) NODE_ENV=development nodemon server.js",
    "server-withstagingdb": "cross-env DATABASE_URL=$(heroku config:get DATABASE_URL -a eventscape-staging) NODE_ENV=development nodemon server.js",
    "client": "npm run start --prefix client",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client",
    "deploy-s3": "cross-env NODE_ENV=production aws s3 sync build/ s3://eventscape-react-app"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "npm": "6.14.4",
    "node": "13.12.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "connect-flash": "^0.1.1",
    "cookie-session": "^1.4.0",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-force-https": "^1.0.0",
    "jwt-simple": "^0.5.6",
    "md5": "^2.3.0",
    "mongoose": "^5.10.9",
    "newrelic": "^7.1.1",
    "node-sass": "^5.0.0",
    "node-schedule": "^1.3.2",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "react-app-polyfill": "^2.0.0",
    "react-favicon": "^0.0.18",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "redis": "^2.8.0",
    "sequelize": "^6.3.5",
    "sequelize-redis": "^1.0.12",
    "socket.io": "^3.0.4",
    "socket.io-redis": "^6.0.1",
    "throng": "^5.0.0",
    "winston": "^3.3.3",
    "wysiwyg-editor-node-sdk": "^3.2.5"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
