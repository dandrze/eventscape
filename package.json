{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "server-dev": "cross-env DATABASE_URL=$(heroku config:get DATABASE_URL -a eventscape-dev) NODE_ENV=development node server.js",
    "server-local": "cross-env DATABASE_URL=postgresql://localhost/eventscapelocal IS_LOCAL=true NODE_ENV=development node server.js",
    "server-prod": "cross-env DATABASE_URL=$(heroku config:get DATABASE_URL -a eventscapeio) NODE_ENV=development node server.js",
    "server-staging": "cross-env DATABASE_URL=$(heroku config:get DATABASE_URL -a eventscape-staging) NODE_ENV=development node server.js",
    "client": "npm run start --prefix client",
    "deploy-s3": "cross-env NODE_ENV=production && npm run build --prefix client && aws s3 sync client/build/ s3://eventscape-react-app-test && aws cloudfront create-invalidation --distribution-id E1K253JNY2FDOQ --paths \"/*\""
  },
